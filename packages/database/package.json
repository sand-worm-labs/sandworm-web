{
  "name": "@sandworm/database",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./dist/index.js",
    "./schemas": "./dist/schemas/index.js"
  },
  "type": "module",
  "scripts": {
    "db:seed": "tsx scripts/seed.ts",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "generate": "drizzle-kit generate:pg",
    "db:generate": "drizzle-kit generate",
    "db:up": "drizzle-kit up",
    "db:check": "drizzle-kit check",
    "db:pull": "drizzle-kit introspect",
    "db:push:danger": "drizzle-kit push",
    "db:delete": "rm -rf drizzle/",
    "prettier": "pnpm exec prettier --write .",
    "lint": "pnpm eslint && pnpm prettier"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.5",
    "next-auth": "^4.2.11",
    "pg": "^8.16.3",
    "zod": "^4.1.11",
    "@faker-js/faker": "^10.0.0",
    "@t3-oss/env-nextjs": "^0.13.8",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@sandworm/eslint-config": "workspace:*",
    "@sandworm/typescript-config": "workspace:*",
    "@types/pg": "^8.15.5",
    "@types/ws": "^8.18.1",
    "drizzle-kit": "^0.31.5",
    "eslint": "8.56.0",
    "tsup": "8.0.2",
    "tsx": "4.7.1",
    "typescript": "5.3.3"
  }
}